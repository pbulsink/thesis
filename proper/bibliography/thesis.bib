	Eprint = {http://pubs.acs.org/doi/pdf/10.1021/om950500e},
	Journal = {Organometallics},
	Number = {1},
	Pages = {236-244},
	Title = {The δ in 18 + δ Electron Complexes:  Importance of the Metal/Ligand Interface for the Substitutional Reactivity of ``Re(0)'' Complexes (α-diimine-)ReI(CO)3(X)},
	Url = {http://pubs.acs.org/doi/abs/10.1021/om950500e},
	Volume = {15},
	Year = {1996},
	Bdsk-Url-1 = {http://pubs.acs.org/doi/abs/10.1021/om950500e},
	Bdsk-Url-2 = {http://dx.doi.org/10.1021/om950500e}}

@article{koike1997,
	Abstract = { Reduction of CO2 to CO was efficiently photocatalyzed by [Re(4,4`-X2bpy)(CO)3PR3]+ (X = H, Me; PR3 = P(OEt)3, P(O-i-Pr)3) in quantum yields of 0.16−0.20. Complexes with CF3 as X or trialkylphosphine as PR3 have much lower photocatalytic ability. One-electron-reduced species of the complexes, which were produced by the photoinduced electron-transfer reaction with triethanolamine, reacted with CO2 in the dark with rate constants of 3.5 × 10-4−1.9 × 10-2 M-1 s-1. The faster the rate of this process, the higher the quantum yield of CO formation. The calculated amount of CO formation, based on the assumption that the process gives CO quantitatively, was similar to the actually observed amount under various conditions. This is consistent with the thermal process being one of the rate-limiting steps in the photocatalyzed reduction of CO2 by the rhenium complexes. },
	Author = {Koike, Kazuhide and Hori, Hisao and Ishizuka, Masakazu and Westwell, Jeremy R. and Takeuchi, Koji and Ibusuki, Takashi and Enjouji, Kengo and Konno, Hideo and Sakamoto, Kazuhiko and Ishitani, Osamu},
