	Issn = {0010-8545},
	Journal = {Coordination Chemistry Reviews},
	Keywords = {Metal carbon stretching vibration},
	Number = {1--2},
	Pages = {211 - 218},
	Title = {Multi-electron reduction of CO2 via Ru-CO2, -C(O)OH, -CO, -CHO, and -CH2OH species},
