	Doi = {10.1039/DT9930000597},
	Issue = {4},
	Journal = {J. Chem. Soc., Dalton Trans.},
	Pages = {597-603},
	Publisher = {The Royal Society of Chemistry},
	Title = {2,2':6',2''-Terpyridine (terpy) acting as a fluxional bidentate ligand. Part 2. Rhenium carbonyl halide complexes, fac-[ReX(CO)3(terpy)](X = Cl, Br or I): NMR studies of their solution dynamics, synthesis of cis-[ReBr(CO)2(terpy)] and the crystal structure of [ReBr(CO)3(terpy)]},
	Url = {http://dx.doi.org/10.1039/DT9930000597},
	Year = {1993},
