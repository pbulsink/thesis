	Doi = {10.1073/pnas.1119863109},
	Eprint = {http://www.pnas.org/content/109/39/15646.full.pdf+html},
	Journal = {Proceedings of the National Academy of Sciences},
	Number = {39},
	Pages = {15646-15650},
	Title = {Kinetic and structural studies, origins of selectivity, and interfacial charge transfer in the artificial photosynthesis of CO},
	Url = {http://www.pnas.org/content/109/39/15646.abstract},
	Volume = {109},
	Year = {2012},
	Bdsk-Url-1 = {http://www.pnas.org/content/109/39/15646.abstract},
