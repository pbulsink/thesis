	Issn = {1099-0682},
	Journal = {European Journal of Inorganic Chemistry},
	Keywords = {Carbonyl ligands, Coincidental resonances, Facial isomers, Energy gap law, Rhenium},
	Number = {1},
	Pages = {118--124},
	Publisher = {WILEY-VCH Verlag},
	Title = {Structural and Photophysical Characterisation of fac-[Tricarbonyl(chloro)(5,6-epoxy-1,10-phenanthroline)rhenium(I)]},
	Url = {http://dx.doi.org/10.1002/ejic.200400531},
	Volume = {2005},
