	Keywords = {charge transfer, luminescence, near-infrared, organic light-emitting diodes, rhenium},
	Number = {40},
	Pages = {13418--13427},
	Publisher = {WILEY-VCH Verlag},
	Title = {Deep Red to Near-Infrared Emitting Rhenium(I) Complexes: Synthesis, Characterization, Electrochemistry, Photophysics, and Electroluminescence Studies},
